package que1stdmarksheet;

import java.util.ArrayList;

public class StudentUtils {
	
	public static ArrayList<Student> populateList() {
		ArrayList<Student> al = new ArrayList<Student>();
		
		al.add(new Student(1,"Ram",99,98,100));
		al.add(new Student(2,"Sham",87,79,92));
		al.add(new Student(3,"Gargi",80,88,95));
		al.add(new Student(4,"Lopamudra",50,50,50));
		al.add(new Student(5,"Maitreya",67,76,85));
		
		return al;
	}

}
